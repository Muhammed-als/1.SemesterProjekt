<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="650.0" maxWidth="600.0" prefHeight="650.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.semesterprojekt.MainMenuController">
    <children>
      <ImageView fitHeight="660.0" fitWidth="612.0">
         <image>
            <Image url="@Background.png" />
         </image>
      </ImageView>
        <Button fx:id="playBTN" cache="true" cacheHint="QUALITY" layoutX="220.0" layoutY="190.0" mnemonicParsing="false" onAction="#switchToGame" opacity="0.7" prefHeight="60.0" prefWidth="160.0" scaleShape="false" text="PLAY">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Segoe UI Symbol" size="19.0" />
         </font></Button>
        <Button fx:id="howBTN" cache="true" cacheHint="QUALITY" depthTest="ENABLE" layoutX="220.0" layoutY="290.0" onAction="#switchToHow" opacity="0.7" prefHeight="60.0" prefWidth="160.0" scaleShape="false" text="HOW">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Segoe UI Symbol" size="19.0" />
         </font></Button>
        <Button fx:id="quitBTN" cache="true" cacheHint="QUALITY" cancelButton="true" depthTest="ENABLE" layoutX="221.0" layoutY="390.0" onAction="#quitBtn" opacity="0.7" prefHeight="60.0" prefWidth="160.0" scaleShape="false" text="QUIT">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Segoe UI Symbol" size="19.0" />
         </font></Button>
      <Label fx:id="titleLabel" layoutX="1.0" layoutY="14.0" prefHeight="18.0" prefWidth="600.0" text="World of Zea" />
      <Label fx:id="nameLabelMain" alignment="CENTER" layoutY="86.0" prefHeight="21.0" prefWidth="600.0">
         <font>
            <Font name="Lucida Console" size="10.0" />
         </font>
      </Label>
    </children>
    <stylesheets>
        <URL value="@application.css" />
    </stylesheets>
   <cursor>
      <Cursor fx:constant="OPEN_HAND" />
   </cursor>
</AnchorPane>
